                    <ColumnDefinition Width="395*"/>
                    <ColumnDefinition Width="659*"/>
                </Grid.ColumnDefinitions>
                <DataGrid x:Name="DgPMaterialesDirectos"  Margin="10,290,10,10" SelectionChanged="DataGrid_SelectionChanged" Grid.ColumnSpan="2"/>
