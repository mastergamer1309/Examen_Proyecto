                        <TextBox x:Name="TxtEntradasDeEfectivo" HorizontalAlignment="Left" Margin="334,98,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="222" FontSize="20" FontFamily="Arial Black"/>
